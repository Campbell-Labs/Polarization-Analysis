classdef SubjectTypes
    %SubjectTypes
    
    properties
        displayString
        subjectClassType
    end
    
    enumeration
        Dog     ('Dog')
        Human   ('Human')
        PureAmyloid ('Pure Amyloid', ArtificalSubject)
    end
    
    methods
        function enum = SubjectTypes(string, classType)
            enum.displayString = string;
            enum.subjectClassType = classType;
        end
    end
    
end

